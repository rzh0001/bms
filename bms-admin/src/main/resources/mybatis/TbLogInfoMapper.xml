<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xj.admin.base.loginfo.mapper.TbLogInfoMapper">
	<!-- 开启二级缓存 -->
	<cache type="org.mybatis.caches.ehcache.LoggingEhcache"/>

	<!-- 通用查询映射结果 -->
	<resultMap id="BaseResultMap" type="com.xj.admin.base.loginfo.entity.TbLogInfo">
		<id column="id" property="id" />
		<result column="user_id" property="userId" />
		<result column="account_name" property="accountName" />
		<result column="method" property="method" />
		<result column="method_args" property="methodArgs" />
		<result column="remark" property="remark" />
		<result column="status" property="status" />
		<result column="operate_ip" property="operateIp" />
		<result column="operate_time" property="operateTime" />
	</resultMap>

</mapper>
