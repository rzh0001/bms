<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xj.bms.bussiness.news.mapper.TbNewsMapper">
	<!-- 开启二级缓存 -->
	<cache type="org.mybatis.caches.ehcache.LoggingEhcache"/>

	<!-- 通用查询映射结果 -->
	<resultMap id="BaseResultMap" type="com.xj.bms.bussiness.news.entity.TbNews">
		<id column="id" property="id" />
		<result column="nav_id" property="navId" />
		<result column="title" property="title" />
		<result column="key_words" property="keyWords" />
		<result column="cover_img" property="coverImg" />
		<result column="is_show" property="isShow" />
		<result column="descs" property="descs" />
		<result column="content" property="content" />
		<result column="create_time" property="createTime" />
		<result column="create_user" property="createUser" />
		<result column="update_time" property="updateTime" />
		<result column="update_user" property="updateUser" />
	</resultMap>

</mapper>
